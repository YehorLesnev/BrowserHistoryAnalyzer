<Window x:Class="BrowserHistoryAnalyzer_WPF.Views.Modals.ErrorMessageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Error" Height="250" Width="350" HorizontalAlignment="Center" VerticalAlignment="Center" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Icon="/Views/Modals/Warning.png">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image Source="/Views/Modals/Warning.png" Width="50" Height="50" Visibility="Visible"></Image>
            <TextBlock x:Name="ErrorMessage" TextWrapping="Wrap">Error message</TextBlock>
        </StackPanel>
    </Grid>
</Window>
