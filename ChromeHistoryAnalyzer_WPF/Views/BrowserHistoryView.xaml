<UserControl x:Class="ChromeHistoryAnalyzer_WPF.Views.BrowserHistoryView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChromeHistoryAnalyzer_WPF.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
            <DataGrid>
                <DataGrid.Columns>
                    <DataGridTextColumn IsReadOnly="True" Header="Id" />
                    <DataGridTextColumn IsReadOnly="True" Header="URL" />
                    <DataGridTextColumn IsReadOnly="True" Header="Title" />
                    <DataGridTextColumn IsReadOnly="True" Header="Last Visited Time" />
                    <DataGridTextColumn IsReadOnly="True" Header="Visit Count" />
                    <DataGridTextColumn IsReadOnly="True" Header="Typed Count" />
                </DataGrid.Columns>
            </DataGrid>
    </Grid>
</UserControl>
